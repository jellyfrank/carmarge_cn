<odoo>
  <data>
    <template id="report_delivery_document" inherit_id="stock.report_delivery_document">
      <xpath expr="//div[@name='div_sched_date']//strong" position="replace">
        <strong>Date:</strong>
      </xpath>
      <xpath expr="//div[@name='div_sched_date']" position="after">
        <div class="col-auto" name="div_origin">
          <strong>Operator:</strong>
          <p t-field="o.create_uid"/>
        </div>
      </xpath>
      <xpath expr="//tr[@t-as='move']" position="before">
        <t t-set="qty" t-value="0"/>
      </xpath>
      <xpath expr="//span[@t-field='move.product_uom_qty']" position="after">
        <t t-set="qty" t-value="qty + move.product_uom_qty"/>
      </xpath>      
      <xpath expr="//tr[@t-as='move']" position="after">
        <tr>
          <td>Total:</td>
          <td> <span t-esc="qty"></span> </td>
        </tr>
      </xpath>
    </template>
  </data>
</odoo>